{
  "version": 2,
  "sql": [
    {
      "engine": "postgresql",
      "queries": "postgresql/queries.sql",
      "schema": "postgresql/schema.sql",
      "gen": {
        "go": {
          "package": "postgresql",
          "out": "postgresql/",
          "sql_package": "pgx/v5",
          "overrides": [
            {
              "db_type": "xid",
              "go_type": {
                "import": "github.com/rs/xid",
                "package": "xid",
                "type": "ID"
              }
            },
            {
              "db_type": "locale",
              "go_type": {
                "import": "libdb.so/hrtclicker/v2/services/user",
                "package": "userservice",
                "type": "Locale"
              }
            },
            {
              "db_type": "compression",
              "go_type": {
                "import": "libdb.so/hrtclicker/v2/services/asset",
                "package": "assetservice",
                "type": "Compression"
              },
              "nullable": true
            },
            {
              "db_type": "notification",
              "go_type": {
                "import": "libdb.so/hrtclicker/v2/services/notification",
                "package": "notificationservice",
                "type": "Notification",
                "pointer": true
              },
              "nullable": true
            },
            {
              "db_type": "notificationconfig",
              "go_type": {
                "import": "libdb.so/hrtclicker/v2/services/notification",
                "package": "notificationservice",
                "type": "NotificationConfigJSON",
                "pointer": true
              },
              "nullable": true
            }
          ]
        }
      }
    }
  ]
}
