{
  "version": 2,
  "sql": [
    {
      "engine": "postgresql",
      "queries": "./postgresqlc/queries",
      "schema": "./postgresqlc/schema",
      "gen": {
        "go": {
          "package": "postgresqlc",
          "out": "postgresqlc/",
          "sql_package": "pgx/v5",
          "initialisms": ["id", "mime"],
          "rename": {
            "dose_ids": "DoseIDs"
          },
          "overrides": [
            {
              "db_type": "usersecret",
              "go_type": {
                "import": "libdb.so/e2clicker/services/user",
                "package": "userservice",
                "type": "Secret"
              }
            },
            {
              "db_type": "locale",
              "go_type": {
                "import": "libdb.so/e2clicker/services/user",
                "package": "userservice",
                "type": "Locale"
              }
            },
            {
              "db_type": "notificationpreferences",
              "go_type": {
                "import": "libdb.so/e2clicker/services/notification",
                "package": "notificationservice",
                "type": "UserPreferences"
              }
            }
          ]
        }
      }
    }
  ]
}
